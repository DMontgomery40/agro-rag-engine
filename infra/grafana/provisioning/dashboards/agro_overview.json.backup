{
  "uid": "agro-overview",
  "slug": "agro-overview",
  "title": "AGRO Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "editable": true,
  "style": "dark",
  "graphTooltip": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "provider",
        "type": "query",
        "label": "Provider",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(agro_requests_total, provider)",
        "refresh": 2,
        "sort": 1,
        "definition": "label_values(agro_requests_total, provider)",
        "hide": 0,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "model",
        "type": "query",
        "label": "Model",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(agro_requests_total{provider=~\"$provider\"}, model)",
        "refresh": 2,
        "definition": "label_values(agro_requests_total{provider=~\"$provider\"}, model)",
        "hide": 0,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "api_filter",
        "type": "textbox",
        "label": "API Filter",
        "query": "/api/",
        "hide": 0
      },
      {
        "name": "system_filter",
        "type": "textbox",
        "label": "System Filter",
        "query": "(docker|qdrant|redis|prometheus)",
        "hide": 0
      },
      {
        "name": "error_filter",
        "type": "textbox",
        "label": "Error Filter",
        "query": "ERROR",
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Request Rate",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 5
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(agro_requests_total{success=\"true\",provider=~\"$provider\",model=~\"$model\"}[5m]))",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.1
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        },
        "overrides": []
      },
      "pluginVersion": "10.0.0"
