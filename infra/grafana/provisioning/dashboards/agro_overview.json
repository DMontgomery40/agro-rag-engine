{
  "uid": "agro-overview",
  "slug": "agro-overview",
  "title": "AGRO Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 3,
  "editable": true,
  "style": "dark",
  "graphTooltip": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "interval",
        "type": "custom",
        "label": "Time Range",
        "query": "5m,1h,6h,24h,7d",
        "options": [
          {
            "text": "5 minutes",
            "value": "5m",
            "selected": false
          },
          {
            "text": "1 hour",
            "value": "1h",
            "selected": false
          },
          {
            "text": "6 hours",
            "value": "6h",
            "selected": false
          },
          {
            "text": "24 hours",
            "value": "24h",
            "selected": true
          },
          {
            "text": "7 days",
            "value": "7d",
            "selected": false
          }
        ],
        "current": {
          "text": "24 hours",
          "value": "24h"
        },
        "hide": 0,
        "includeAll": false,
        "multi": false
      },
      {
        "name": "provider",
        "type": "query",
        "label": "Provider",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(agro_tokens_total, provider)",
        "refresh": 2,
        "sort": 1,
        "definition": "label_values(agro_tokens_total, provider)",
        "hide": 0,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "model",
        "type": "query",
        "label": "Model",
        "datasource": {
          "type": "prometheus",
          "uid": "PBFA97CFB590B2093"
        },
        "query": "label_values(agro_tokens_total{provider=~\"$provider\"}, model)",
        "refresh": 2,
        "definition": "label_values(agro_tokens_total{provider=~\"$provider\"}, model)",
        "hide": 0,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "api_filter",
        "type": "textbox",
        "label": "API Filter",
        "query": "/api/",
        "hide": 0
      },
      {
        "name": "system_filter",
        "type": "textbox",
        "label": "System Filter",
        "query": "(docker|qdrant|redis|prometheus)",
        "hide": 0
      },
      {
        "name": "error_filter",
        "type": "textbox",
        "label": "Error Filter",
        "query": "ERROR",
        "hide": 0
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Request Rate",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 5
      },
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "expr": "sum(rate(agro_requests_total{success=\"true\"}[$interval])) * 3600",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "value",
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqph",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "green",
                "value": 1000
              }
            ]
          }
        },
        "overrides": []
      },
      "pluginVersion": "10.0.0"
    },
    {
