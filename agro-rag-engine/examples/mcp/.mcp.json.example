{
  "mcpServers": {
    "rag-service": {
      "command": "/absolute/path/to/agro/.venv/bin/python",
      "args": [
        "/absolute/path/to/agro/mcp_server.py"
      ],
      "env": {
        "OPENAI_API_KEY": "your-openai-key-here",
        "QDRANT_URL": "http://127.0.0.1:6333",
        "REDIS_URL": "redis://127.0.0.1:6379/0",
        "COHERE_API_KEY": "your-cohere-key-here-optional",
        "VOYAGE_API_KEY": "your-voyage-key-here-optional",
        "NETLIFY_API_KEY": "your-netlify-key-here-optional",
        "GEN_MODEL": "qwen3-coder:30b",
        "RERANK_BACKEND": "local",
        "RERANKER_MODEL": "BAAI/bge-reranker-v2-m3",
        "EMBEDDING_TYPE": "openai",
        "OLLAMA_URL": "http://127.0.0.1:11434/api"
      }
    }
  }
}
